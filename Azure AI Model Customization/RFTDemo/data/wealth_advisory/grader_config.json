{
    "name":"wealth_advisory_grader",
    "type":"multi",
    "graders":{
      "meets_criteria":{
        "name":"meets_criteria",
        "type":"string_check",
        "input":"{{item.reference_answer.meets_criteria}}",
        "operation":"eq",
        "reference":"{{sample.output_json.meets_criteria}}"
      },
      "descriptive_analysis":{
        "name":"descriptive_analysis",
        "type":"string_check",
        "input":"{{item.reference_answer.descriptive_analysis}}",
        "operation":"eq",
        "reference":"{{sample.output_json.descriptive_analysis}}"
      }
    },
    "calculate_output":"0.5*meets_criteria + 0.5 * descriptive_analysis"
}
  